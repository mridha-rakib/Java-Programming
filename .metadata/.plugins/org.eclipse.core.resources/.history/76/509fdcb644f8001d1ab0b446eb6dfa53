package DataStructure;

import java.util.*;
import java.lang.*;
import java.io.*;

public class ZeroSum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc = new Scanner( System.in );
		
		int t = sc.nextInt();
		
		while ( t-->0 )
		{
			int n = sc.nextInt();
			
			int arr[] = new int[n];
			
			for ( int i = 0; i < n; i++ )
			{
				arr[i] = sc.nextInt();
			}
			
			System.out.println(findsum(arr, n));
		}

	}

}
