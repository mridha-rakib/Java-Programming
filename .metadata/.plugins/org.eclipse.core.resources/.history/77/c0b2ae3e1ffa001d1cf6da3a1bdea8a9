package DataStructure;

public class MajorityElement {
	
	public static int majorityElement( int[] v )
	{
		int n = v.length;
		
		for ( int i = 0; i < n; i++ )
		{
			
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		 int[] arr = {2, 2, 1, 1, 1, 2, 2};
